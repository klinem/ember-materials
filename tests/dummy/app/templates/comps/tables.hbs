<section class="doc-section" id="examples">
  <h2 class="doc-section-title">
    Examples
  </h2>

  <div class="doc-example">
    <div class="md-card">
      <table class="md-table">
        <thead>
          <tr>
            <th>Dessert (100g serving)</th>
            <th>Calories</th>
            <th>Fat (g)</th>
            <th>Carbs (g)</th>
            <th>Protein (g)</th>
            <th>Sodium (mg)</th>
            <th>Calcium (%)</th>
            <th>Iron (%)</th>
          </tr>
        </thead>
        <tbody>
          {{#each model as |t|}}
            <tr>
              <td>{{t.name}}</td>
              <td>{{t.calories}}</td>
              <td>{{t.fat}}</td>
              <td>{{t.carbs}}</td>
              <td>{{t.protein}}</td>
              <td>{{t.sodium}}</td>
              <td>{{t.calcium}}</td>
              <td>{{t.iron}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</section>
