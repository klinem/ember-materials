<section class="doc-section" id="examples">
  <h2 class="doc-section-title">
    Examples
  </h2>

  <div class="doc-example">
    <div class="md-card">
      <table class="md-table">
        {{#md-table-header}}
          {{md-table-th text="Dessert (100g serving)" sortBy="name"}}
          {{md-table-th text="Calories"}}
          {{md-table-th text="Fat (g)"}}
          {{md-table-th text="Carbs (g)"}}
          {{md-table-th text="Protein (g)"}}
          {{md-table-th text="Sodium (mg)"}}
          {{md-table-th text="Calcium (%)"}}
          {{md-table-th text="Iron (%)"}}
        {{/md-table-header}}
        <tbody>
          {{#each model as |t|}}
            <tr>
              <td>{{t.name}}</td>
              <td>{{t.calories}}</td>
              <td>{{t.fat}}</td>
              <td>{{t.carbs}}</td>
              <td>{{t.protein}}</td>
              <td>{{t.sodium}}</td>
              <td>{{t.calcium}}</td>
              <td>{{t.iron}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</section>
