<section class="doc-section" id="examples">
  <h2 class="doc-section-title">
    Examples
  </h2>

  <div class="doc-example">
    <div class="md-card">
      <div class="card-body">
        <div class="">
          {{#md-button class="default"}}
            Default
          {{/md-button}}
          {{#md-button class="primary"}}
            Primary
          {{/md-button}}
          {{#md-button class="warning"}}
            Warning
          {{/md-button}}
          {{#md-button class="danger"}}
            Danger
          {{/md-button}}
          {{#md-button class="default" disabled=true}}
            Disabled
          {{/md-button}}
        </div>
        <div>
          {{#md-button class="default raised"}}
            Default
          {{/md-button}}
          {{#md-button class="primary raised"}}
            Primary
          {{/md-button}}
          {{#md-button class="warning raised"}}
            Warning
          {{/md-button}}
          {{#md-button class="danger raised"}}
            Danger
          {{/md-button}}
          {{#md-button class="default raised" disabled=true}}
            Disabled
          {{/md-button}}
        </div>
      </div>
    </div>
  </div>
</section>
